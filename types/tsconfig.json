{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "next-auth": ["."],
      "next-auth/providers": ["./providers"],
      "next-auth/adapters": ["./adapters"],
      "next-auth/react": ["./react-client"],
      "next-auth/jwt": ["./jwt"]
    }
  }
}
