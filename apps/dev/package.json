    "nodemailer": "^6",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^7",
    "cpx": "^1.5.0",
    "fake-smtp-server": "^0.8.0",
    "prisma": "^3"
  }
