collection Account {
    index byUserId {
        terms [.userId]
    }
    index byProviderAndProviderAccountId {
        terms [.provider, .providerAccountId]
    }
}

collection CustomAccount {
    index byUserId {
        terms [.userId]
    }
    index byProviderAndProviderAccountId {
        terms [.provider, .providerAccountId]
    }
}

