{
  "name": "@auth/nuxt",
  "description": "Authentication for Nuxt.",
  "version": "0.3.0",
  "keywords": [
    "Nuxt",
    "oauth",
    "jwt",
    "oauth2",
    "authentication",
    "csrf",
    "oidc",
    "Auth.js"
  ],
  "author": "",
  "contributors": [],
  "repository": "https://github.com/nextauthjs/next-auth",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf dist src/providers",
    "providers": "node ../utils/scripts/providers",
    "build": "pnpm clean && pnpm providers && nuxi prepare && nuxt-module-build build",
    "prepack": "nuxi prepare && nuxt-module-build build",
    "test": "vitest run -c ../utils/vitest.config.ts"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "eslint-plugin-vue": "^9.28.0",
    "h3": "^1.12.0",
    "nuxt": "^3.13.2",
    "ofetch": "^1.4.0",
    "rimraf": "^6.0.1",
    "unbuild": "^2.0.0",
    "vue-eslint-parser": "^9.4.3",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "4.0.0-next.53"
  },
  "dependencies": {
    "@auth/core": "workspace:*",
    "@nuxt/kit": "^3.13.2",
    "defu": "^6.1.4",
    "vue": "^3.5.10"
  },
  "type": "module",
  "types": "./dist/types.d.ts",
  "module": "./dist/module.mjs",
  "files": [
    "dist",
    "*.d.ts",
    "*.mjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "default": "./dist/module.mjs",
      "import": {
        "types": "./dist/module.d.mts",
        "default": "./dist/module.mjs"
      }
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts"
    },
    "./providers/*": {
      "types": "./dist/providers/*.d.ts",
      "import": "./dist/providers/*.js"
    }
  }
}
