{
  "name": "@auth/core",
  "version": "0.37.0",
  "description": "Authentication for the Web.",
  "keywords": [
    "authentication",
    "authjs",
    "jwt",
    "oauth",
    "oidc",
    "passwordless",
    "standard",
    "vanilla",
    "webapi"
  ],
  "homepage": "https://authjs.dev",
  "repository": "https://github.com/nextauthjs/next-auth.git",
  "author": "Bal치zs Orb치n <info@balazsorban.com>",
  "contributors": [
    "Bal치zs Orb치n <info@balazsorban.com>",
    "Nico Domino <yo@ndo.dev>",
    "Lluis Agusti <hi@llu.lu>",
    "Thang Huu Vu <hi@thvu.dev>",
    "Iain Collins <me@iaincollins.com>"
  ],
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters.d.ts",
      "import": "./dist/adapters.js",
      "default": "./dist/adapters.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js",
      "default": "./dist/errors.js"
    },
    "./jwt": {
      "types": "./dist/jwt.d.ts",
      "import": "./dist/jwt.js",
      "default": "./dist/jwt.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts"
    },
    "./providers/*": {
      "types": "./dist/providers/*.d.ts",
      "import": "./dist/providers/*.js",
      "default": "./dist/providers/*.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@panva/hkdf": "^1.2.1",
    "@types/cookie": "0.6.0",
    "cookie": "0.7.1",
    "jose": "^5.9.3",
    "oauth4webapi": "^3.0.0",
    "preact": "10.11.3",
    "preact-render-to-string": "5.2.3"
  },
  "peerDependencies": {
    "@simplewebauthn/browser": "^9.0.1",
    "@simplewebauthn/server": "^9.0.2",
    "nodemailer": "^6.8.0"
  },
  "peerDependenciesMeta": {
    "@simplewebauthn/browser": {
      "optional": true
    },
    "@simplewebauthn/server": {
      "optional": true
    },
    "nodemailer": {
      "optional": true
    }
  },
  "scripts": {
    "build": "pnpm css && pnpm providers && tsc",
    "clean": "rm -rf dist",
    "css": "node scripts/generate-css",
    "dev": "pnpm css && pnpm providers && tsc -w",
    "test": "vitest run -c ../utils/vitest.config.ts",
    "test:watch": "vitest -c ../utils/vitest.config.ts",
    "providers": "node scripts/generate-providers"
  },
  "devDependencies": {
    "@simplewebauthn/browser": "9.0.1",
    "@simplewebauthn/server": "9.0.3",
    "@simplewebauthn/types": "^9.0.1",
    "@types/node": "18.11.10",
    "@types/nodemailer": "6.4.6",
    "@types/react": "18.0.37",
    "autoprefixer": "10.4.13",
    "postcss": "8.4.19",
    "postcss-nesting": "^12.0.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "4.0.0-next.53"
  }
}
