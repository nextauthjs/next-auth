{
  "name": "@auth/sveltekit",
  "version": "0.8.0",
  "description": "Authentication for SvelteKit.",
  "keywords": [
    "authentication",
    "authjs",
    "jwt",
    "sveltekit",
    "oauth",
    "oidc",
    "passwordless",
    "svelte"
  ],
  "homepage": "https://sveltekit.authjs.dev",
  "repository": "https://github.com/nextauthjs/next-auth.git",
  "author": "Thang Huu Vu <hi@thvu.dev>",
  "contributors": [
    "Thang Huu Vu <hi@thvu.dev>",
    "Balázs Orbán <info@balazsorban.com>",
    "Nico Domino <yo@ndo.dev>",
    "Lluis Agusti <hi@llu.lu>",
    "Iain Collins <me@iaincollins.com>"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm providers && pnpm check && svelte-package && node ./scripts/postbuild.js && rm -rf package",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "clean": "rm -rf client.* index.* package src/lib/providers",
    "dev": "svelte-package -w",
    "preview": "vite preview",
    "providers": "node ../utils/scripts/providers.js --out src/lib",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "1.29.2",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.4.3",
    "@sveltejs/package": "^1.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/set-cookie-parser": "^2.4.7",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@auth/core": "workspace:*",
    "set-cookie-parser": "^2.6.0"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0 || ^2.0.0",
    "svelte": "^3.54.0 || ^4.0.0 || ^5"
  },
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "client.*",
    "index.*",
    "providers",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./client": {
      "types": "./client.d.ts",
      "import": "./client.js"
    },
    "./adapters": {
      "types": "./adapters.d.ts"
    },
    "./providers": {
      "types": "./providers/index.d.ts"
    },
    "./providers/*": {
      "types": "./providers/*.d.ts",
      "import": "./providers/*.js"
    },
    "./package.json": "./package.json"
  }
}
